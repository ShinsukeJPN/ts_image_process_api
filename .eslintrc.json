{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "standard"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "no-void": "off",
        "camelcase": "off",
        "no-sequences": "off",
        "no-sparse-arrays": "off",
        "no-return-assign": "off"
    }
    
}
